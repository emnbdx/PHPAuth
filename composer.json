{
    "name": "emnbdx/phpauth",
    "description": "Fork of PHPAuth for custom usage",
    "type": "library",
    "keywords": [
        "authentication",
        "class",
        "secure"
    ],
    "homepage": "https://phpauth.github.io/PHPAuth/",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "phpmailer/phpmailer": "^6.0"
    },
    "suggest": {
        "phpauth/phpauth.l10n": "^0.1.0",
        "phpauth/phpauth.email-validator": "^0.1.1",
        "ganlvtech/kcaptcha": "^1.0",
        "bjeavons/zxcvbn-php": "^1.0",
        "google/recaptcha": "^1.2",
        "phpmailer/phpmailer": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "PHPAuth\\": "sources/"
        }
    }
}